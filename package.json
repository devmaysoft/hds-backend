{
  "name": "hds",
  "version": "1.0.0",
  "private": true,
  "description": "backend for mock design company",
  "license": "ISC",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean:dist",
    "build": "next build",
    "clean:dist": "rm -rf .next",
    "predev": "npm run clean:dist",
    "dev": "next",
    "lint": "tsc --noEmit && eslint src --ext .ts,.tsx",
    "prisma2:generate": "prisma2 generate",
    "prisma2:init": "prisma2 migrate save --name 'init' --experimental && prisma2 migrate up --experimental --verbose && prisma2 generate",
    "prisma2:migrate-save": "prisma2 migrate save --experimental",
    "prisma2:migrate-up": "prisma2 migrate up --experimental --verbose",
    "prisma2:preview": "prisma2 migrate save --experimental --preview",
    "seed": "npm run seed:build && npm run seed:run",
    "seed:build": "webpack --config ./data/webpack.config.js --node-args=\"--experimental-modules\"",
    "seed:run": "node ./.dist/main.js --experimental-modules",
    "start": "next start",
    "test": "snyk test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@material-ui/core": "^4.9.7",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@prisma/client": "^2.0.0-preview024",
    "axios": "^0.19.2",
    "cookie": "^0.4.0",
    "dotenv": "^8.2.0",
    "jsonwebtoken": "^8.5.1",
    "lodash.some": "^4.6.0",
    "next": "canary",
    "nookies": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.1.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-jsx": "^7.9.1",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.1",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/cookie": "^0.3.3",
    "@types/dotenv": "^8.2.0",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash.some": "^4.6.6",
    "@types/node": "^13.9.2",
    "@types/react": "^16.9.25",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "body-parser": "^1.19.0",
    "commitizen": "^4.0.3",
    "confusing-browser-globals": "^1.0.9",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-prettier-standard": "^3.0.1",
    "eslint-config-standard": "^14.1.1",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "prettier-config-standard": "^1.0.1",
    "prisma2": "^2.0.0-preview024",
    "sass": "^1.26.3",
    "snyk": "^1.301.0",
    "ts-node": "^8.7.0",
    "typescript": "^3.9.0-dev.20200321",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  }
}
